printWidth: 80
semi: false
singleQuote: true
proseWrap: "never"
arrowParens: "avoid"
bracketSpacing: true
endOfLine: "auto"
eslintIntegration: true
htmlWhitespaceSensitivity: "ignore" # 属性标签格式化符合预期
ignorePath: ".prettierignore"
jsxBracketSameLine: false
jsxSingleQuote: false
stylelintIntegration: true
trailingComma: "none"
tslintIntegration: false
vueIndentScriptAndStyle: false
embeddedLanguageFormatting: "auto"
overrides:
  -
    files: ".prettierrc"
    options:
      parser: "json"

